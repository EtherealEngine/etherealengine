{
  "name": "@xr3ngine/clientbuild",
  "version": "0.2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/xr3ngine/xr3ngine.git"
  },
  "engines": {
    "yarn": ">= 0.18.0"
  },
  "npmClient": "yarn",
  "jest": "ts-jest",
  "scripts": {
    "copyfiles-client": "copyfiles design/** design/**/* public/** public/**/* **/*.scss lib/client",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=--max_old_space_size=4096 node ./index.js --port 3000",
    "build": "NODE_OPTIONS=--max_old_space_size=4096 next build ./",
    "prestart": "webpack --config ./library.webpack.config.js",
    "start": "next start",
    "watch": "cross-env NODE_ENV=development next --port 3001 --hostname localhost",
    "validate": "yarn run lint && yarn run test",
    "test": "echo \"TODO: Add Testing to Client\" && exit 0",
    "lint": "node node_modules/.bin/eslint --fix --quiet '*.ts' '**/*.ts' '**/*.tsx' && node node_modules/.bin/stylelint '**/*.scss'"
  },
  "resolutions": {
    "webpack": "^5.1.0"
  },
  "dependencies": {
    "babel-loader": "^8.1.0",
    "next": "9.5.4",
    "next-images": "^1.4.0",
    "next-optimized-images": "^2.6.2",
    "react": "^16.12.0",
    "three": "^0.121.1"
  },
  "devDependencies": {
    "cache-loader": "^4.1.0",
    "thread-loader": "^3.0.0",
    "webpack": "^5.1.0",
    "webpack-cli": "^4.0.0"
  },
  "license": "ISC"
}
